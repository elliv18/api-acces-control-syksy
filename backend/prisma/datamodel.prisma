enum UserType {
  ADMIN
  USER
}


type User {
    id: ID! @id
    userType: UserType!
    email: String! @unique
    password: String!
    apiKeysId: [UserApiKeys] @relation(name: "ApiKeys") 
    createdAt: DateTime! @createdAt
    updatedAt: DateTime! @updatedAt
}

type UserApiKeys {
    id: ID! @id
    hash: String
    userId: User! @relation(name: "Apikeys")
    createdAt: DateTime! @createdAt
    updatedAt: DateTime! @updatedAt
}